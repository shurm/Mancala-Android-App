<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.boss.mike4shur.mancalaapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mancala_icon_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mancala_icon_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.boss.mike4shur.mancalaapp.MainActivity" android:screenOrientation="sensorLandscape">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.boss.mike4shur.mancalaapp.PlayingMancalaActivity" android:screenOrientation="sensorLandscape">

        </activity>
        <activity android:name="com.boss.mike4shur.mancalaapp.HowToPlayActivity" android:screenOrientation="sensorLandscape">

        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>